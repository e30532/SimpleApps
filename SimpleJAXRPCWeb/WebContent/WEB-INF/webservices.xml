<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd" version="1.3">
	<webservice-description>
		<webservice-description-name>HelloService</webservice-description-name>
		<wsdl-file>WEB-INF/wsdl/Hello.wsdl</wsdl-file>
		<jaxrpc-mapping-file>WEB-INF/Hello_mapping.xml</jaxrpc-mapping-file>
		<port-component>
			<port-component-name>Hello</port-component-name>
			<wsdl-port xmlns:pfx="http://jaxrpc.simple">
			pfx:Hello</wsdl-port>
			<enable-mtom>false</enable-mtom>
			<service-endpoint-interface>simple.jaxrpc.Hello</service-endpoint-interface>
			<service-impl-bean>
				<servlet-link>
				simple_jaxrpc_Hello</servlet-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>
